{
  "expo": {
    "name": "chingoo",
    "slug": "chingoo",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chingoo.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.chingoo.app"
    },
    "scheme": "chingoo",
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font",
      "expo-dev-client"
    ]
  }
}
